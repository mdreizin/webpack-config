{
  "env": {
    "test": {
      "plugins": [
        "istanbul"
      ]
    }
  },
  "presets": [
    ["env", {
      "targets": {
        "node": 6
      }
    }]
  ],
  "plugins": [
    ["babel-plugin-transform-builtin-extend", {
      "globals": [
        "Array"
      ]
    }],
    "transform-runtime"
  ],
  "only": [
    "src/",
    "test/",
    "jasmine.js"
  ]
}
